@{
}
<form method="post">
    <div class="row mb-4">
        <div class="col">
            <div class="form-outline">
                <input type="text" id="PrinteryName" name="PrinteryName" class="form-control" />
                <label class="form-label" for="PrinteryName">Printery Name</label>
            </div>
            <div class="form-outline">
                <input type="text" id="PrinteryDescription" name="PrinteryDescription" class="form-control" />
                <label class="form-label" for="PrinteryDescription">Printery Description</label>
            </div>
            <div class="form-outline">
                <input type="text" id="PrinteryMail" name="PrinteryMail" class="form-control" />
                <label class="form-label" for="PrinteryMail">Printery Mail</label>
            </div>
        </div>
        <div class="col">
            <div class="form-outline">
                <input type="text" id="PrinteryAdress" name="PrinteryAdress" class="form-control" />
                <label class="form-label" for="PrinteryAdress">Printery Adress</label>
            </div>
            <div class="form-outline">
                <input type="text" id="PrinteryPhone" name="PrinteryPhone" class="form-control" />
                <label class="form-label" for="PrinteryName">Printery Phone</label>
            </div>
            <div class="form-outline">
                <input type="text" id="PrinteryIBAN" name="PrinteryIBAN" class="form-control" />
                <label class="form-label" for="PrinteryIBAN">Printery IBAN</label>
            </div>
        </div>
    </div>
    <!-- Submit button -->
    <button type="submit" class="btn btn-primary">Register</button>
</form>